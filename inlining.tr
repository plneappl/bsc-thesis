start S
begin
  S = S
  S_1 = collapse S A_1
  in
    A  -> A_1 tif E1:1 tthen A:2 S2 | A_3 tt:4 
    S2 -> S2_1 te A:3 | S2_2 "" 
  out
    S -> S_1 tif E:1 tthen S:2
       | S_2 tif E:1 tthen S:2 te S:3
       | S_3 tt:4 
  pattern auto
end
begin
  E = E
  in
    E1 -> E1_1 x | E1_2 y
  seq
    E -> E_1 x | E_2 y
  pattern auto
end
