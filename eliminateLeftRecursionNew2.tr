start A
begin
  S1 = A
  begin
    S1_1 = collapse S1 S_1 
    in
    S -> S_1 S t F | S_2 F
    out
    S1 -> S1_1 F R
    R  -> R_1 t S1 | R_2 "" 
    pattern
    S_2 x = S1_1 x y
    S_1 (S_2 x) "+" y  = S1_1 x (S_2 y)
    S_1 (S1_1 x y) t z = S1_1 x (S_1 y t z)
  end
  begin
    S1 = F
    S1_1 = collapse S1 A_1
    in
    A -> A_1 <int>
    out
    S1 -> S1_1 <int>
    pattern
    A_1 x = S1_1 x
  end
end

